    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "tsx": "^3.12.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
